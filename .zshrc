source ~/.zsh/prompt
source ~/.zsh/completition
source ~/.zsh/keybinds
source ~/.zsh/aliases
source ~/.zsh/startup

autoload -Uz compinit
compinit

# Lines configured by zsh-newuser-install
HISTFILE=~/.histfile
HISTSIZE=1000
SAVEHIST=1000
setopt autocd extendedglob notify
unsetopt beep
bindkey -e
# End of lines configured by zsh-newuser-install

